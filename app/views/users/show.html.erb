<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Password:</strong>
  <%= @user.password %>
</p>

<p>
  <strong>Salt:</strong>
  <%= @user.salt %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
<table>
<thead>
    <tr>
      <th>filnavn</th>
      <th>filtype</th>
      <th>size</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<tbody>
    <% Upload.where(user_id: session[:id]) do |file| %>
      <tr>
        <td><%= file.filnavn %></td>
        <td><%= user.filtype %></td>
        <td><%= user.size %></td>
      </tr>
    <% end %>
  </tbody>
</table>